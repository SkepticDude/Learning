    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "Python Compiler",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal",
            "justMyCode": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Node Compiler",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}